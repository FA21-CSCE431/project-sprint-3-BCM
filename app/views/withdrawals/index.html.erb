<p id="notice"><%= notice %></p>

<h1>Withdrawals</h1>

<table>
  <thead>
    <tr>
      <th>Withdraw</th>
      <th>Officer uin</th>
      <th>Transaction</th>
      <th>Amount</th>
      <th>Title</th>
      <th>Description</th>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @withdrawals.each do |withdrawal| %>
      <tr>
        <td><%= withdrawal.withdraw_id %></td>
        <td><%= withdrawal.officer_uin %></td>
        <td><%= withdrawal.transaction_id %></td>
        <td><%= withdrawal.amount %></td>
        <td><%= withdrawal.title %></td>
        <td><%= withdrawal.description %></td>
        <td><%= withdrawal.date %></td>
        <td><%= link_to 'Show', withdrawal %></td>
        <td><%= link_to 'Edit', edit_withdrawal_path(withdrawal) %></td>
        <td><%= link_to 'Destroy', withdrawal, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Withdrawal', new_withdrawal_path %>
